@{
    ViewBag.Title = "Client Feedback";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <div class="row">
        <div class="col-lg-12">
            <div class="btn-group btn-breadcrumb">
                <a href="~/Admin/Index" class="btn btn-primary" onclick="Loader()"><i class="fa fa-tachometer"></i> Dashboard</a>
                <a href="#" class="btn btn-primary-child" style="pointer-events:none"><i class="fa fa-comment"></i> Client Feedback</a>
                <a href="#" class="btn btn-primary" style="display:none"></a>
            </div>
        </div>
    </div>
        </h1>
    </div>
</div>
<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
  <thead>
    <tr style="background:linear-gradient(to right, #7fc645 0%,#048ed2 100%) !important;color:#fff;text-align:center">
      <th>S.No</th>
      <th>Feedback By</th>
      <th>Feedback</th>
      <th>Created On</th>
    </tr>
  </thead>
  <tbody>
      @{
          int i = 1;
      foreach (var item in Model) {
    <tr> 
      <td>@i</td>
      <td>@item.FeedbackBy</td>
      <td>@item.Feedback</td>
      <td>@item.CreatedDate</td>
    </tr>
          i = i + 1;
      }
      }
  </tbody>
</table>

<script>
    $(document).ready(function () {
        $('#example').DataTable();
    });
</script>