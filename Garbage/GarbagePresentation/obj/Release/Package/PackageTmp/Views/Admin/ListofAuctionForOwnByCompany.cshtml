@{
    ViewBag.Title = "Own Auction List";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <div class="row">
                <div class="col-lg-12">
                    <div class="btn-group btn-breadcrumb">
                        <a class="btn btn-primary" href="~/Admin/Index" onclick="Loader()"><i class="fa fa-tachometer"></i> Dashboard</a>
                        <a class="btn btn-primary-child" href="#" style="pointer-events: none"><i class="fa fa-list-alt"></i> Own Auction List</a>
                        <a class="btn btn-primary" href="#" style="display: none"></a>
                    </div>
                </div>
            </div>
        </h1>
    </div>
</div>
<table id="example" cellspacing="0" class="table table-striped table-bordered" width="100%">
    <thead>
        <tr style="background: linear-gradient(to right, #7fc645 0%,#048ed2 100%) !important; color: #fff; text-align: center">
            <th>Auction Address</th>
            <th>Detail</th>
            <th>Estimated Rate</th>
            <th>Date From</th>
            <th>Time From</th>
            <th>Date To</th>
            <th>Time To</th>
            <th>Status</th>
            <th>Assign To</th>
            <th>Contact No</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
            <tr>
                <td>@item.Address</td>
                <td>@item.Detail</td>
                <td>@item.EstimatedRate</td>
                <td>@item.DateFrom</td>
                <td>@item.TimeFrom</td>
                <td>@item.DateTo</td>
                <td>@item.TimeTo</td>
                <td>@item.Status</td>
                <td>@item.Name</td>
                <td>@item.ContactNo</td>
            </tr>
      }
    </tbody>
</table>
<script>
    $(document).ready(function () {
        $('#example').DataTable();
    });
</script>